        new MockServerClient("localhost", 1080)
        .when(
                request()
                        .withMethod("GET")
                        .withPath("/azuki/rest/customer/Sanoma/businessunit/SBS/asset/Medium130_dbzyr6/media/Medium130_dbzyr6/mediatranscodeformat/EBS_tf_hls_phone_tablet_hd_enc_1_0/masterplaylist")
                        .withHeaders(
                                new Header("Accept-Encoding", "gzip, deflate"),
                                new Header("Authorization", "Basic ZW1wX3Zhc3Rwcm94eV9zeXN0ZW1fdXNlcjo0bFYlUHFTeXA5XX0lclZ6VTc1WWpIS2hTMzRmMnlfLDktcTFBOF13JFpiVHlNL1B4RQ=="),
                                new Header("Host", "0.0.0.0"),
                                new Header("User-Agent", "RxNetty Client"),
                                new Header("Content-Length", "0")
                        ),
                Times.once()
        )
        .respond(
                response()
                        .withStatusCode(200)
                        .withHeaders(
                                new Header("X-MULE_SESSION", ""),
                                new Header("Server", "nginx"),
